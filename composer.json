{
    "name": "thavarshan/storable",
    "type": "package",
    "description": "File Storabled key, value storage system. ",
    "keywords": ["php", "settings", "json"],
    "authors": [
        {
            "name": "Thavarshan Thayananthajothy",
            "email": "tjthavarshan@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.3",
        "friendsofphp/php-cs-fixer": "^2.16.3",
        "symfony/var-dumper": "^5.1.2"
    },
    "autoload": {
        "psr-4": {
            "Storable\\": "src/Storable"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Storable\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "scripts": {
        "test": "phpunit --colors=always --verbose=true"
    }
}
