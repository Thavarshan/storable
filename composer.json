{
    "name": "thavarshan/storable",
    "type": "package",
    "description": "File Storabled key, value storage system. ",
    "keywords": ["php", "settings", "json"],
    "authors": [
        {
            "name": "Thavarshan Thayananthajothy",
            "email": "tjthavarshan@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Storable\\": "src/Storable"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Storable\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always --verbose"
    }
}
